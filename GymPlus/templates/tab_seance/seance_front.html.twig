
{% extends 'base.html.twig' %}

{% block title %}TabSeance index{% endblock %}

{% block body %}
    <h1>TabSeance index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdSeance</th>
                <th>TypeSeance</th>
                <th>DateDebut</th>
                <th>DateFin</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% if tab_seances is defined %}
        {% for tab_seance in tab_seances %}
            <tr>
                <td>{{ tab_seance.idSeance }}</td>
                <td>{{ tab_seance.typeSeance }}</td>
                <td>{{ tab_seance.dateDebut }}</td>
                <td>{{ tab_seance.dateFin }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %} 
{% endif %}
        </tbody>
    </table>

   
{% endblock %}

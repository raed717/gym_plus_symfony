{% extends 'base.html.twig' %}

 
{% block body %}

{% block stylesheets %}
        <link href="{{ asset('public/templates/frontseance/style.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}
    <table class="table">
        <thead>
            <tr>
                <th>IdSeance</th>
                <th>TypeSeance</th>
                <th>DateDebut</th>
                <th>DateFin</th>
                
            </tr>
        </thead>
        <tbody>
       
        {% for TabSeance in TabSeances %}
            <tr>
                <td>{{ TabSeance.idSeance }}</td>
                <td>{{ TabSeance.typeSeance }}</td>
                <td>{{ TabSeance.dateDebut }}</td>
                <td>{{ TabSeance.dateFin }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %} 

        </tbody>
    </table>
    <div class="navigation">
            {{ knp_pagination_render(TabSeances) }}
        </div>

   
{% endblock %}


